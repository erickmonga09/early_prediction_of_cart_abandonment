    "# Function to load, process, and test a single test set\n",
    "def process_and_test_single_set(n, oracle, group_id):\n",
    "    file_path = f\"./test_sessions_{n}.json\"\n",
    "\n",
    "    # Load and pre-process the test set\n",
    "    with open(file_path, 'r') as file:\n",
    "        test_data = json.load(file)\n",
    "    # Convert the JSON data to a list\n",
    "    test_data = list(test_data.values())\n",
    "\n",
    "    # Make predictions on the unseen dataset using your Oracle model\n",
    "    ypred = [int(predict_with_oracle(session, oracle)) for session in test_data]\n",
    "\n",
    "    # Make predictions using the trained model\n",
    "    session_ids = list(range(len(ypred)))\n",
    "    predictions_dict = dict(zip(session_ids, map(int, ypred)))\n",
    "    \n",
    "    # Save the predictions as a JSON file\n",
    "    output_file = f\"predictions_group{group_id}_oracle_at{n}.json\"\n",
    "    with open(output_file, 'w') as file:\n",
    "        json.dump(predictions_dict, file)\n",
    "\n",
    "    # Load the true labels\n",
    "    labels_file_path = f\"./test_labels_{n}.json\"\n",
    "    with open(labels_file_path, 'r') as file:\n",
    "        y_true = json.load(file)\n",
    "    y_true = [value for _, value in y_true.items()]\n",
    "    \n",
    "    # Calculate accuracy and display classification report\n",
    "    accuracy_rf = accuracy_score(y_true, ypred)\n",
    "    print(f\"Test label {n}\")\n",
    "    print(f\"Accuracy: {accuracy_rf:.2f}\")\n",
    "    print(\"\\nClassification Report:\")\n",
    "    print(classification_report(y_true, ypred, zero_division=1))\n",
    "    print(\"\\nConfusion Matrix:\")\n",
    "    print(confusion_matrix(y_true, ypred))\n",
    "    print(\"\\n\")\n",
    "\n",
    "# Process and test each test set separately\n",
    "group_id = '9'\n",
    "for n in [5, 10, 15]:\n",
    "    process_and_test_single_set(n, oracle, group_id)"
