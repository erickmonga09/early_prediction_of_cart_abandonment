    "# Get the data\n",
    "sessions = df['session'].to_list()\n",
    "labels = df['purchase'].to_list()  \n",
    "train_ids = np.random.choice(\n",
    "    len(symbolised_sessions), int(np.floor(len(symbolised_sessions) * 0.7)), replace=False\n",
    ")\n",
    "\n",
    "# Split the data\n",
    "training_sessions, training_labels, eval_sessions, eval_labels = [], [], [], []\n",
    "for idx, (session, label) in enumerate(zip(symbolised_sessions, labels)):\n",
    "    if idx in train_ids:\n",
    "        training_sessions.append(session)\n",
    "        training_labels.append(label)\n",
    "    else:\n",
    "        eval_sessions.append(session)\n",
    "        eval_labels.append(label)\n",
    "\n",
    "# Preprocess\n",
    "d = defaultdict(lambda: defaultdict(int))\n",
    "for session, label in zip(training_sessions, training_labels):\n",
    "    d[str(tuple(session))][label] += 1\n",
    "\n",
    "# Train the model\n",
    "oracle = {}\n",
    "for session in d.keys():\n",
    "    n_buy, n_ws = 0, 0\n",
    "    for label in d[session].keys():\n",
    "        if label:\n",
    "            n_buy += d[session][label]\n",
    "        else:\n",
    "            n_ws += d[session][label]\n",
    "    oracle[session] = n_buy / (n_buy + n_ws)\n",
    "\n",
    "# Prediction function\n",
    "def predict_with_oracle(session, oracle, default_prediction=0):\n",
    "    session_tuple_str = str(tuple(session))\n",
    "    if session_tuple_str in oracle:\n",
    "        return oracle[session_tuple_str]\n",
    "    else:\n",
    "        return default_prediction\n",
    "\n",
    "# Make predictions on the unseen dataset using your Oracle model\n",
    "ypred = [int(predict_with_oracle(session, oracle)) for session in eval_sessions]\n",
    "\n",
    "# Calculate F1 score and accuracy\n",
    "f1oracle = f1_score(eval_labels, ypred, average='binary')\n",
    "accuracyoracle = accuracy_score(eval_labels, ypred)\n",
    "\n",
    "print(f\"F1 score: {f1oracle}\")\n",
    "print(f\"Accuracy: {accuracyoracle}\")"
   ]
