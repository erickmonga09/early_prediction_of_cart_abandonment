    "# Function to load, process, and test a single test set\n",
    "def process_and_test_single_set(n, classifier, group_id, X_test):\n",
    "    # Make predictions using the trained model\n",
    "    predictions = classifier.predict(X_test)\n",
    "    session_ids = list(range(len(predictions)))\n",
    "    predictions_dict = dict(zip(session_ids, map(int, predictions)))\n",
    "    \n",
    "    # Save the predictions as a JSON file\n",
    "    output_file = f\"predictions_group{group_id}_expModel_rf_at{n}.json\"\n",
    "    with open(output_file, 'w') as file:\n",
    "        json.dump(predictions_dict, file)\n",
    "\n",
    "    # Load the true labels\n",
    "    labels_file_path = f\"./test_labels_{n}.json\"\n",
    "    with open(labels_file_path, 'r') as file:\n",
    "        y_true = json.load(file)\n",
    "    y_true = [value for _, value in y_true.items()]\n",
    "        \n",
    "    # Calculate accuracy and display classification report\n",
    "    accuracy_rf = accuracy_score(y_true, predictions)\n",
    "    print(f\"Test label {n}\")\n",
    "    print(f\"Accuracy: {accuracy_rf:.2f}\")\n",
    "    print(\"\\nClassification Report:\")\n",
    "    print(classification_report(y_true, predictions, zero_division=1))\n",
    "    print(\"\\nConfusion Matrix:\")\n",
    "    print(confusion_matrix(y_true, predictions))\n",
    "\n",
    "# Instantiate the best RandomForestClassifier\n",
    "best_rf = RandomForestClassifier(random_state = 42, criterion = 'gini', verbose = 1, n_jobs = -1, \n",
    "                                 class_weight = 'balanced_subsample', max_depth = 15, max_features = None, min_samples_leaf = 1000, min_samples_split = 5000, n_estimators = 200)\n",
    "\n",
    "# Train the RF on the full dataset\n",
    "X_rf = df_train_full.drop(columns=['session_id_hash', 'session', 'purchase'])\n",
    "y_rf = df_train_full['purchase']\n",
    "best_rf.fit(X_rf, y_rf)\n",
    "\n",
    "# Process and test each test set separately\n",
    "group_id = '9'\n",
    "for n in [5, 10, 15]:\n",
    "    if n == 5:\n",
    "        test_set = df_test_5.copy()\n",
    "    elif n == 10:\n",
