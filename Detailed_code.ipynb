    "# Function to load, process, and test a single test set\n",
    "def process_and_test_single_set(n, nn_model, rf_model, group_id, percentile_99, rf_data):\n",
    "    ############################### Neural network ###############################\n",
    "    file_path = f\"./test_sessions_{n}.json\"\n",
    "\n",
    "    # Load the test set\n",
    "    with open(file_path, 'r') as file:\n",
    "        test_data = json.load(file)\n",
    "    # Convert the JSON data to a DataFrame\n",
    "    test_set = pd.DataFrame(test_data.items(), columns=['session_id', 'session'])\n",
    "\n",
    "    # Preprocess the data for the neural network\n",
    "    max_seq_length = int(percentile_99) # Only 1% of the data will lose some information\n",
    "    encoder = OneHotEncoder(sparse_output=False, categories=[range(0, 5)])  # Explicitly specify categories 0 to 4\n",
    "    padded_sequences = pad_sequences(test_set['session'].tolist(), maxlen=max_seq_length, padding='post', truncating='post')\n",
    "\n",
    "    # Flatten, one-hot encode, and then reshape back to 3D\n",
    "    flat_sequences = padded_sequences.reshape(-1, 1)\n",
    "    one_hot_flat_sequences = encoder.fit_transform(flat_sequences)\n",
    "    num_classes = one_hot_flat_sequences.shape[1]\n",
    "    one_hot_sequences = one_hot_flat_sequences.reshape(-1, max_seq_length, num_classes)\n",
    "\n",
    "    # Predict probability for Neural Network\n",
    "    nn_probs = nn_model.predict(one_hot_sequences)\n",
    "    nn_probs = np.array(nn_probs)\n",
    "    nn_probs = np.squeeze(nn_probs)\n",
    "\n",
    "    ############################### Random Forest ###############################\n",
    "\n",
    "    # Predict probability for Random Forest\n",
    "    rf_probs = rf_model.predict_proba(rf_data)[:, 1]\n",
    "\n",
    "    ############################### Ensemble ###############################\n",
    "\n",
    "    # Combine the predictions\n",
    "    combined_probs = (rf_probs + nn_probs) / 2\n",
    "\n",
    "    # Set a threshold\n",
    "    threshold = 0.3\n",
    "    combined_preds = (combined_probs > threshold).astype(int)\n",
    "\n",
    "    # Save the predictions as a JSON file\n",
    "    output_file = f\"predictions_group{group_id}_expModel_ensemble_at{n}.json\"\n",
    "    with open(output_file, 'w') as file:\n",
