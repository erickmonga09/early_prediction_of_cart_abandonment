   "source": [
    "def generate_features(preprocessed_data: pd.DataFrame) -> pd.DataFrame:\n",
    "    \"\"\"\n",
    "    Generate 12 new features and apply a Box-Cox transformation on each of them.\n",
    "    \n",
    "    input:  preprocessed_data: A pre-processed DataFrame containing the filtered sessions.\n",
    "    return: The input DataFrame with 12 new features added.\n",
    "    \"\"\"\n",
    "\n",
    "    # Feature 1: session_duration\n",
    "    preprocessed_data['session_duration'] = preprocessed_data.groupby('session_id_hash')['server_timestamp_epoch_ms'].transform(lambda x: x.max() - x.min())\n",
    "\n",
    "    # Feature 2: total_pageviews\n",
    "    preprocessed_data['total_pageviews'] = preprocessed_data.groupby('session_id_hash')['event_type'].transform(lambda x: (x == 'pageview').sum())\n",
    "\n",
    "    # Feature 3: total_events\n",
    "    preprocessed_data['total_events'] = preprocessed_data.groupby('session_id_hash')['event_type'].transform('count')\n",
    "\n",
    "    # Feature 4: total_product_events\n",
    "    preprocessed_data['total_product_events'] = preprocessed_data.groupby('session_id_hash')['event_type'].transform(lambda x: (x == 'event_product').sum())\n",
    "\n",
    "    # Feature 5: detail_views\n",
    "    preprocessed_data['detail_views'] = preprocessed_data.groupby('session_id_hash')['product_action'].transform(lambda x: (x == 'detail').sum())\n",
    "\n",
    "    # Feature 6: unique_products_viewed\n",
    "    preprocessed_data['unique_products_viewed'] = preprocessed_data.groupby('session_id_hash')['product_sku_hash'].transform('nunique')\n",
    "\n",
    "    # Feature 7: cart_adds\n",
    "    preprocessed_data['cart_adds'] = preprocessed_data.groupby('session_id_hash')['product_action'].transform(lambda x: (x == 'add').sum())\n",
    "\n",
    "    # Feature 8: cart_removes\n",
    "    preprocessed_data['cart_removes'] = preprocessed_data.groupby('session_id_hash')['product_action'].transform(lambda x: (x == 'remove').sum())\n",
    "\n",
    "    # Feature 9: unique_urls\n",
    "    preprocessed_data['unique_urls'] = preprocessed_data.groupby('session_id_hash')['hashed_url'].transform('nunique')\n",
    "\n",
    "    # Feature 10: product_view_ratio\n",
    "    preprocessed_data['product_view_ratio'] = preprocessed_data['total_product_events'] / preprocessed_data['total_pageviews']\n",
    "\n",
    "    # Feature 11: add_to_detail_ratio\n",
    "    preprocessed_data['add_to_detail_ratio'] = preprocessed_data['cart_adds'] / preprocessed_data['detail_views']\n",
    "\n",
    "    # Feature 12: remove_to_detail_ratio\n",
    "    preprocessed_data['remove_to_detail_ratio'] = preprocessed_data['cart_removes'] / preprocessed_data['detail_views']\n",
    "\n",
    "    # Define list of columns to transform\n",
    "    columns_to_transform = ['session_duration', 'total_pageviews', 'total_events', 'total_product_events',\n",
    "                            'detail_views', 'unique_products_viewed', 'cart_adds', 'cart_removes', \n",
    "                            'unique_urls', 'product_view_ratio', 'add_to_detail_ratio', 'remove_to_detail_ratio']\n",
    "    \n",
    "    # Apply Box-Cox transformation to each column\n",
    "    for col in columns_to_transform:\n",
    "        preprocessed_data[col] = boxcox(preprocessed_data[col] + 1)[0]  # Add 1 to avoid zero-values\n",
    "    \n",
    "    return preprocessed_data\n",
    "\n",
    "# Copy df_test into df_test_features\n",
    "df_train_features = df_raw.copy()\n",
    "\n",
